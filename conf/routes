# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET      /bioeditor/gene/:gene/statements                           org.nextprot.web.statements.controllers.BioEditorApplication.geneStatements(gene: String, report ?= null)
GET      /bioeditor/statements                           org.nextprot.web.statements.controllers.BioEditorApplication.statements(report ?= null)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file)
